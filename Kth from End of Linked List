https://www.geeksforgeeks.org/problems/nth-node-from-end-of-linked-list/1
/* struct Node {
  int data;
  struct Node *next;
  Node(int x) {
    data = x;
    next = NULL;
  }
};*/

// Function to find the data of kth node from the end of a linked list.
class Solution {
  public:
    int getKthFromLast(Node *head, int k) {
        // Your code here
        Node *temp=head;
        int n=1;
        while(temp->next!=NULL)
        {
            temp=temp->next;
            n++;
        }
        temp=head;
        int cnt=1;
        while(temp!=NULL)
        {
            if(cnt==n-k+1)
                return temp->data;
            temp=temp->next;
            cnt++;
        }
        return -1;
    }
};
