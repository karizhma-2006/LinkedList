https://www.geeksforgeeks.org/problems/finding-middle-element-in-a-linked-list/1
class Solution {
  public:
    /* Should return data of middle node. If linked list is empty, then -1 */
    int getMiddle(Node* head) {
        // code here
        Node *temp=head;
        int cnt=1;
        while(temp->next!=NULL)
        {
            temp=temp->next;
            cnt++;
        }
       cnt=(cnt/2)+1;
        temp=head;
        while(cnt>1)
        {
            temp=temp->next;
            cnt--;
        }
        return temp->data;
    }
};
